#include <stdio.h>

int main()
{
    int n;
    printf("Enter the year: ");
    scanf("%d",&n);
    if(n%100==0)
    {
        if(n%400==0)
        {
            printf("It is a century leap year");
        }
        else
        {
            printf("It is a century year but not a leap year");
        }
    }
    else if(n%4==0)
    {
        printf("It is a Leap year");
    }
    else
    {
        printf("It is nOT a Leap year");
    }
}



OUTPUT:
Enter the number:1700
It is a century year but not a leap year
